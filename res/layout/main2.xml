<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/main_background"
    >

<ImageView android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:background="#00000000" 
		android:layout_marginLeft="5pt" 
		android:layout_marginTop="5pt" /> 



<RelativeLayout 
	android:layout_height="wrap_content" 
	android:id="@+id/RelativeLayout01" 
	android:layout_width="wrap_content"
	android:background="#00000000"
	android:paddingTop="120dp"
>

<LinearLayout
	android:id="@+id/line"
    android:layout_width="fill_parent"
    android:layout_height="85dip"
    android:gravity="center_horizontal"
>
<ImageButton 
	android:id="@+id/IBcampus" 
	android:layout_height="85dip" 
	android:layout_width="72dip" 
	android:layout_gravity="center"
	android:background="@drawable/arview_clicked"
	android:layout_marginRight="30dip"
	>
</ImageButton>

<ImageButton 
	android:layout_height="85dip" 
	android:layout_width="72dip" 
	android:layout_gravity="center"
	android:id="@+id/IBcoupon"
	android:background="@drawable/coupon_clicked"
	android:layout_marginRight="30dip"
	>
</ImageButton>

<ImageButton 
	android:id="@+id/IBtwitter" 
	android:layout_height="85dip" 
	android:layout_width="72dip" 
	android:layout_gravity="center"
	android:background="@drawable/twitter_clicked"
	android:layout_marginRight="30dip"
	>
</ImageButton>
<ImageButton 
	android:background="@drawable/library_clicked" 
	android:id="@+id/IBlib" 
	android:layout_height="85dip" 
	android:layout_width="72dip" 
	android:layout_gravity="center"></ImageButton>
</LinearLayout>

<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:layout_below="@id/line"
  
>

<ImageButton 
	android:id="@+id/IBbus" 
	android:layout_height="85dip" 
	android:layout_width="72dip" 
	android:layout_gravity="center"
	android:background="@drawable/bus"
	android:layout_marginRight="30dip"
	>
</ImageButton>




<ImageButton 
	android:id="@+id/IBrest" 
	android:layout_height="85dip" 
	android:layout_width="72dip" 
	android:layout_gravity="center"
	android:background="@drawable/restaurant"
	android:layout_marginRight="30dip"
	>
</ImageButton>

<ImageButton 
	android:id="@+id/IBschedule" 
	android:layout_height="85dip" 
	android:layout_width="72dip" 
	android:layout_gravity="center"
	android:background="@drawable/timetable"
	android:layout_marginRight="30dip"
	>
</ImageButton>

<ImageButton 
	android:id="@+id/IBinfo"
	android:layout_height="85dip" 
	android:layout_width="72dip" 
	android:layout_gravity="center"
	android:background="@drawable/smunews"
	>
</ImageButton>
</LinearLayout>
</RelativeLayout>
</LinearLayout>
</FrameLayout>
